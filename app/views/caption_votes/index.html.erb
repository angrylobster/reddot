<p id="notice"><%= notice %></p>

<h1>Caption Votes</h1>

<table>
  <thead>
    <tr>
      <th>Vote</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @caption_votes.each do |caption_vote| %>
      <tr>
        <td><%= caption_vote.vote %></td>
        <td><%= link_to 'Show', caption_vote %></td>
        <td><%= link_to 'Edit', edit_caption_vote_path(caption_vote) %></td>
        <td><%= link_to 'Destroy', caption_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Caption Vote', new_caption_vote_path %>
